"*":
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "linter"
    ]
    excludeVcsIgnoredPaths: false
    packagesWithKeymapsDisabled: []
    telemetryConsent: "limited"
  editor:
    fontFamily: "Operator Mono"
    fontSize: 15
    preferredLineLength: 100000000
    showIndentGuide: true
    tabType: "soft"
  "exception-reporting":
    userId: "f1a6ed5f-713c-4a54-859a-805517785979"
  "git-diff": {}
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  nuclide:
    "nuclide-diagnostics-ui": {}
    "nuclide-distraction-free-mode":
      hideFindAndReplace: true
      hideStatusBar: true
    "nuclide-flow":
      canUseFlowBin: true
      warnOnNotInstalled: false
    "nuclide-hg-repository": {}
    "nuclide-home":
      showHome: false
    "nuclide-working-sets": {}
    use:
      "nuclide-file-tree": false
  "one-dark-ui": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
    silenceErrors: true
    useEslint: true
  "tool-bar":
    position: "Left"
  welcome:
    showOnStartup: false
